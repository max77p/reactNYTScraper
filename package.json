{
  "name": "reactNYTSearch",
  "version": "1.0.0",
  "description": "NYT scraper and article saver using Reactjs",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "client": "cd client && yarn start",
    "server":"nodemon server.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/max77p/reactNYTScraper.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/max77p/reactNYTScraper/issues"
  },
  "homepage": "https://github.com/max77p/reactNYTScraper#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.2.13",
    "nodemon": "^1.18.4",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {},
  "engines": {
    "node": "8.10.0",
    "npm": "6.4.0"
  }
}
